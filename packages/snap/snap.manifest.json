{
  "version": "0.1.0",
  "description": "A MetaMask snap the wraps ERC-4337 eth namespace methods.",
  "proposedName": "ERC-4337 Relayer",
  "repository": {
    "type": "git",
    "url": "https://github.com/transeptorlabs/erc-4337-snap.git"
  },
  "source": {
    "shasum": "LV5X+HUT7Ub7JWkfVO77EonsGMFw+q+VzToNoD0E0Vo=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "images/icon.svg",
        "packageName": "@transeptor/erc-4337-relayer",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
