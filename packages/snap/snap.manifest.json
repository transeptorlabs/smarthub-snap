    "endowment:network-access": {},
    "snap_manageState": {},
    "endowment:cronjob": {
      "jobs": [
        {
          "expression": "* * * * *",
          "request": {
            "method": "checkUserOperationReceiptReady"
          }
        }
      ]
    }
  },
  "manifestVersion": "0.1"
